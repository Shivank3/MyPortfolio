{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Information"
        }
    },
    "Cors": {
        "AllowedOrigins": [
            "http://localhost:4200"
        ]
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=PortfolioDb;User Id=sa;Password=your_password;"
    },
    "Swagger": {
        "Enable": true
    },
    "FeatureFlags": {
        "EnableExperimentalFeatures": true
    },
    "ApiSettings": {
        "ExternalApiBaseUrl": "https://api.example.com"
    },
    "AppSettings": {
        "ApplicationName": "Portfolio API (Development)",
        "Version": "1.0.0-dev"
    }
}
